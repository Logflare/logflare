<script>
    $('.navbar-toggler').click(function () {
        var atratribut = $('.navbar-toggler').attr('aria-expanded');
        if (atratribut == "false") {


            $(".barsic").css("display", "none");
            $(".xicon").css("display", "block");
        } else {
            $(".barsic").css("display", "block");
            $(".xicon").css("display", "none");
